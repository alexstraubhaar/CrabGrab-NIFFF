
package ch.hearc.coursjava.moo.classInterneAnonyme;



public class Maison
	{

	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/

	public Maison(int prix, int surface)
		{
		this.prix = prix;
		this.surface = surface;
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	@Override
	public String toString()
		{
		StringBuilder builder = new StringBuilder();
		builder.append("Maison [prix=");
		builder.append(this.prix);
		builder.append(", surface=");
		builder.append(this.surface);
		builder.append("]");
		return builder.toString();
		}

	/*------------------------------*\
	|*				Set				*|
	\*------------------------------*/

	/*------------------------------*\
	|*				Get				*|
	\*------------------------------*/

	public int getPrix()
		{
		return this.prix;
		}

	public int getSurface()
		{
		return this.surface;
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/

	//input/output
	private int prix;




	private int surface;
	}

