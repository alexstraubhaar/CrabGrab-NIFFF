
package ch.hearc.coursjava.moo.enumeration.quadratic;

import java.util.Map;

/*
 *
 */

public class UseQuadratic
	{

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	public static void main(String[] args)
		{
		main();
		}

	public static void main()
		{
		Map<String, Double> mapABC = parser();
		}

	private static Map<String, Double> parser()
		{
		String a = System.getProperty("a");
		String b = System.getProperty("b");
		String c = System.getProperty("c");

		toDouble(a);

		return null;


		}

	private static void toDouble(String str)
		{
		try
			{
			Double.valueOf(str);
			}
		catch (NumberFormatException e)
			{
			// TODO Auto-generated catch block
			e.printStackTrace();
			}

		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	}
